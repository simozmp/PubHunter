      <Rectangle style="-fx-fill: rgb(0,0,0,.7)" height="640.0" layoutX="20.0" layoutY="210.0" width="360.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="80.0" />
