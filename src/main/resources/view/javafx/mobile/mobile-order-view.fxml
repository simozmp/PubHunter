         <Pane prefHeight="0.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
         <Button fx:id="dismissErrorButton" styleClass="accent-flat-button" mnemonicParsing="false" prefWidth="250.0" text="Dismiss" />
      </VBox>
